 package dec19_simple_program;
// Find the evenodd number
 public class OddEven {

	public static void main(String[] args) {
		
		int[] a = {1,2,3,4,5,6,7};
		
		System.out.print("These are even number ");
		for(int i=0; i<a.length;i++)
		{
			if(a[i]%2==0)
			{
				System.out.print(a[i]+" ");
			}
		}
		System.out.println();
		System.out.print("These are odd number ");
		
			for(int i=0; i<a.length;i++)
			{
				if(a[i]%2!=0)
				{
					System.out.print(a[i]+" ");
				}
			
			}
	}
	}


